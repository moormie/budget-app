    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "firebase": "^9.14.0",
    "moment": "^2.29.4",
    "postcss": "^8.0.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
